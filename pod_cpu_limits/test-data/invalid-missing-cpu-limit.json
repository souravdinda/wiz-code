{
  "kind": "Deployment",
  "metadata": {
    "name": "web-app",
    "annotations": {}
  },
  "spec": {
    "template": {
      "spec": {
        "containers": [{
          "name": "nginx",
          "image": "nginx:1.21",
          "resources": {
            "limits": {
              "memory": "512Mi"
            },
            "requests": {
              "cpu": "250m",
              "memory": "256Mi"
            }
          }
        }]
      }
    }
  }
}

