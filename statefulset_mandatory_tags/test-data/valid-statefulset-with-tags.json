{
  "kind": "StatefulSet",
  "metadata": {
    "name": "web-app",
    "namespace": "production",
    "labels": {
      "env": "production",
      "owner": "platform-team",
      "name": "web-app"
    }
  },
  "spec": {
    "template": {
      "spec": {
        "containers": [
          {
            "name": "nginx",
            "image": "nginx:1.21"
          }
        ]
      }
    },
    "serviceName": "web-app",
    "replicas": 3
  }
}